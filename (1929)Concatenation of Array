class Solution {
    public int[] getConcatenation(int[] nums) {
        int[] a=new int[2*nums.length];
        int j=0;
        for(int i=0;i<nums.length;i++)
        {
            a[i]=nums[i];
            j++;
        }
        for(int i=0;i<nums.length;i++)
        {
            a[j]=nums[i];
            j++;
        }
        return a;
    }
}
